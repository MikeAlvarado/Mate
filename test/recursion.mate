programa Recursion {
  funcion fibonacci(iteracion) {
    si(iteracion <= 1) {
      regresa iteracion;
    }
    regresa fibonacci(iteracion-1) + fibonacci(iteracion-2);
  }
  funcion origen {
    escribe("Escribe el numero de iteraciones de fibonacci");
    var numero = lee();
    escribe(fibonacci(numero));
  }
}