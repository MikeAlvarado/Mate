programa FactorialIterativo {
  funcion operacion_factorial (numero) {
    si (numero <= 1) {
      regresa 1;
    }
    var i = 1, resultado = 1;
    mientras (i <= numero) {
      resultado = resultado * i;
      i = i + 1;
    }
    regresa resultado;
  }

  funcion origen {
    mientras (cierto) {
      escribe("Escribe un nÃºmero factorial");
      var numero = lee();
      escribe("Resultado: " + operacion_factorial(numero));
    }
  }
}